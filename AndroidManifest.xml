<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:a="http://schemas.android.com/apk/res/android"
      package="com.giago.referraltester"
      a:versionCode="4"
      a:versionName="1.1.1">
    <application a:icon="@drawable/icon" a:label="@string/app_name" a:theme="@style/ReferralTester">
        <activity a:name=".ReferralTester"
                  a:label="@string/app_name" a:configChanges="touchscreen|keyboard|keyboardHidden|orientation">
            <intent-filter>
                <action a:name="android.intent.action.MAIN" />
                <category a:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity a:name=".Send"
                  a:label="@string/app_name"
                  a:configChanges="touchscreen|keyboard|keyboardHidden|orientation"/>
    </application>
    <uses-sdk a:minSdkVersion="4" />
    <uses-permission a:name="android.permission.READ_LOGS" />
</manifest>